{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/api-docs",
      "dest": "/server.js"
    },
    {
      "src": "/webhook",
      "dest": "/server.js"
    },
    {
      "src": "/health",
      "dest": "/server.js"
    },
    {
      "src": "/thumbnail",
      "dest": "/server.js"
    },
    {
      "src": "/generate-thumbnail",
      "dest": "/server.js"
    },
    {
      "src": "/setup-webhook",
      "dest": "/server.js"
    },
    {
      "source": "/update-commands",
      "destination": "/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ],
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  }
}
